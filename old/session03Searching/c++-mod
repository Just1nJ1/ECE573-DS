#include <iostream>
#include <random>
#include <cmath>
using namespace std;

const double phi = (1+sqrt(5))/2; // ~1.618

int goldenMeanSearch(int data[], int n) {
    int L = 0, R = n-1;
    int S = (R-L) / phi;
    int x = R - S, y = L + s;

    if (data[x] > data[y]) {
       R = y;
       y = x;
       S = (R-L) / phi;
       x = R - S;
       }   
}